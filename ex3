#include <iostream>
#include <stdio.h>
using namespace std;

int main()
{
     int myAge=15;
    int myWeight=50;
    int myHeight=165;
    int mySisters=1;
     int myBrothers=0;
    printf("I am %d year old \n;",myAge);
    printf("my weight %d kg \n;",50);
    printf("my height is %d cm \n;",165);
    printf("I have %d sisters %d brothers \n;",1,0);
    
    cout << "I am "<< 15 <<"yers old";
    cout << "my weight is "<< 50 <<"kg";
    cout << "my height is "<< 165 <<"cm";
    cout << "I have "<< 1 <<"sisters"<< 0 <<"brothers";
   
   
    return 0;
}
